; or1k data bus master
[master dbus]
slaves =
 eth0
 lms_mem
 mem0_dbus
 myriadrf
 uart0

[slave lms_mem]
size=0x4 000 000
offset=0x4000000

[slave mem0_dbus]
offset=0x00000000
size=0x4000000

[slave uart0]
datawidth=8
offset=0x90000000
size=32

[slave eth0]
offset=0x92000000
size=4096

[slave myriadrf]
offset=0x95000000
size=0x1000
